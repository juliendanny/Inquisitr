<table>
  <tr>
    <th>Title</th>
  </tr>

<% @questions.each do |question| %>
  <tr>
    <td><%= link_to question.title, question %></td>
    <td>Asked by <%= question.user.username %></td>
    <td><%= time_ago_in_words(question.created_at) %> ago</td>
    <td><%= pluralize(question.answers.count, "Answer") %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Question', new_question_path %>
